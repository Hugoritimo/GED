import React from "react";

// Definição do tipo para um documento
interface DocumentType {
  id: string | number;
  title: string;
}

// Tipagem para as props do componente
interface DocumentSectionAdvancedProps {
  documents: DocumentType[];
  onSelectDocument: (doc: DocumentType) => void;
  onEditDocument: (doc: DocumentType) => void;
}

// *** Esta é a sintaxe correta que não causa o erro de build ***
const DocumentSectionAdvanced = ({
  documents,
  onSelectDocument,
  onEditDocument,
}: DocumentSectionAdvancedProps) => {

  const handleEditClick = (
    event: React.MouseEvent<HTMLButtonElement>,
    document: DocumentType
  ) => {
    event.stopPropagation();
    onEditDocument(document);
  };

  return (
    <div>
      {documents.map((document) => (
        <div key={document.id} onClick={() => onSelectDocument(document)}>
          <h3>{document.title}</h3>
          <button onClick={(event) => handleEditClick(event, document)}>
            Edit
          </button>
        </div>
      ))}
    </div>
  );
};

export default DocumentSectionAdvanced;